{
  "name": "hongkong",
  "version": "1.3.0",
  "description": "jQuery based plugin for parallax scrolling",
  "main": "hongkong.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "grunt test",
    "lint": "eslint {,test/}*.js",
    "watch": "webpack --watch",
    "build": "webpack",
    "build:test": "webpack --config webpack-test.config.js",
    "lite": "lite-server",
    "start": "concurrently \"npm run watch\" \"npm run lite\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drublic/hongkong.git"
  },
  "keywords": [
    "parallax",
    "scrolling"
  ],
  "author": "Hans Christian Reinl <info@drublic.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drublic/hongkong/issues"
  },
  "homepage": "https://github.com/drublic/hongkong",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.0",
    "grunt": "1.0.4",
    "grunt-contrib-jasmine": "2.0.3",
    "grunt-contrib-jshint": "2.1.0",
    "lite-server": "^2.5.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "eslint": "^6.0.0",
    "jquery": "^3.4.1",
    "normalize.css": "^8.0.1",
    "uglifyjs": "^2.4.11"
  }
}
